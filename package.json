{
  "name": "rentCarApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --simulator='iPhone 13'",
    "android": "react-native run-android",
    "start": "react-native start",
    "prepare": "husky install",
    "postinstall": "patch-package",
    "check-config": "npx --quiet react-native config",
    "patch-package": "yarn add patch-package postinstall-postinstall",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && yarn cache clean --force",
    "kill:java": "killall -9 java",
    "reset": "yarn start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate:bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "keygen": "keytool -genkeypair -v -keystore android/app/getride-key.keystore -alias getride -keyalg RSA -keysize 2048 -validity 10000",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:dev": "ENVFILE=.env.development && react-native run-android --variant=developmentDebug",
    "android:dev-release": "ENVFILE=.env.development && react-native run-android --variant=developmentrelease",
    "android:stg": "ENVFILE=.env.staging && react-native run-android --variant=stagingdebug",
    "android:stg-release": "ENVFILE=.env.staging && react-native run-android --variant=stagingrelease",
    "android:prod": "ENVFILE=.env.production && react-native run-android --variant=productionDebug",
    "android:prod-release": "ENVFILE=.env.production && react-native run-android --variant=productionrelease",
    "android:dev-apk": "ENVFILE=.env.development && cd android && ./gradlew assembleDevelopmentRelease && killall -9",
    "android:stg-apk": "ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease",
    "android:prod-apk": "ENVFILE=.env.production && cd android && ./gradlew assembleProductionRelease && killall -9 java",
    "android:prod-aab": "ENVFILE=.env.production && cd android && ./gradlew bundleProductionRelease && killall -9 java",
    "android:bundle": "cd android && ENVFILE=.env.production ./gradlew bundleRelease && cd ..",
    "setDevelopment": "ENVFILE=.env.development",
    "setStaging": "ENVFILE=.env.staging",
    "setProduction": "ENVFILE=.env.production",
    "android:stg-deploy": "yarn android:clean && cd android && fastlane android deploy_staging && cd ..",
    "android:prod-deploy": "cyarn android:clean && cd android && fastlane android deploy_production && cd ..",
    "ios:Pod:Reset": "cd ios && pod deintegrate && pod setup && pod install",
    "ios:clean": "yarn node:clean &&  cd ios && rm -rf ~/Library/Caches/CocoaPods && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && yarn ios:Pod:Reset",
    "ios:Pod:install": "cd ios && pod install && cd ..",
    "ios:bundle:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:dev": "yarn setDevelopment  react-native run-ios --mode=Debug --scheme \"development\"",
    "ios:dev-release": "yarn ios:clean && yarn setDevelopment  react-native run-ios --mode=Release --scheme \"development\"",
    "ios:staging": "yarn setStaging  react-native run-ios --scheme \"staging\"",
    "ios:staging-release": "yarn ios:clean && yarn setStaging  react-native run-ios --mode=Release --scheme \"staging\"",
    "ios:prod": "yarn setProduction  react-native run-ios --mode=Debug --scheme \"production\"",
    "appcenter:android-dev": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-Android -d Development -m --disable-duplicate-release-error",
    "appcenter:android-prod": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-Android -d Production -m --disable-duplicate-release-error",
    "appcenter:android-stg": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-Android -d Staging -m --disable-duplicate-release-error",
    "appcenter:ios-dev": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-iOS -d Development -m --disable-duplicate-release-error",
    "appcenter:ios-prod": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-iOS -d Production -m --disable-duplicate-release-error",
    "appcenter:ios-stg": "appcenter codepush release-react -a antoni_luthfi3/Get-Ride-User-iOS -d Staging -m --disable-duplicate-release-error",
    "revopush:android-dev": "revopush release-react get_ride_customer android -d Development -m --disable-duplicate-release-error"
  },
